#include <iostream>

using namespace std;

int main()
{
    int a=0;
    int b=0;
    string ciag;
    cout << "Program zliczajacy ilosc liczb calkowitych conajmniej dwucyfrowych w ciagu" <<endl;
    ciag = "xy12zab123xiy";
    cout << "Dany jest ciag: " << endl;
    cout<<ciag<<endl;
    for(int i=0; i<ciag.length(); i++)
    {
        if(ciag.at(i)>47&&ciag.at(i)<59)
        {
            a=a+1;
        }
        else
        {
            a=0;
        }
        if(a==2)
        {
            b=b+1;
        }
    }
    cout <<"Ilosc liczb dwucyfrowych w ciagu:" << b << endl;
    return 0;
}
